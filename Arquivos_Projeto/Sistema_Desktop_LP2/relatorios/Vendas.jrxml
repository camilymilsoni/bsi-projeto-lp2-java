	<queryString>
		<![CDATA[SELECT
    v.id_vend,
    p.id_prod,
    p.nome_prod,
    pv.val_prod,
    pv.qtd_prod,
    v.val_vend,
    c.id_cli,
    c.nome_cli
FROM venda v
INNER JOIN produto_venda pv ON v.id_vend = pv.id_vend
INNER JOIN produto p ON pv.id_prod = p.id_prod
INNER JOIN cliente c ON v.id_cli = c.id_cli
]]>
